# Example Development Environment Variables
# Copy this file to .env.development and customize the values

# Authentication bypass for development (true/false)
VITE_DEV_BYPASS_AUTH=true

# Debug and development features (true/false)
VITE_DEBUG_MODE=true
VITE_ENABLE_MOCK_DATA=true

# API endpoints for development
VITE_API_BASE_URL=http://localhost:8080

# Supabase configuration
# Replace with your actual Supabase project credentials
VITE_SUPABASE_URL=your-supabase-url
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key

# Feature flags (true/false)
VITE_ENABLE_CHAT_ASSISTANT=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_SOCIAL_MEDIA=true
VITE_ENABLE_ERROR_TRACKING=true
VITE_ENABLE_PERFORMANCE_MONITORING=true